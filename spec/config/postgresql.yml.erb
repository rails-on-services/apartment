connections:
  postgresql:
    adapter: postgresql
    database: apartment_postgresql_test
    username: postgres
    min_messages: WARNING
    host: localhost
    port: 5432
    <% if defined?(JRUBY_VERSION) %>
      driver: org.postgresql.Driver
      url: jdbc:postgresql://localhost:5432/apartment_postgresql_test
      timeout: 5000
      pool: 5
    <% else %>
      schema_search_path: public
      password:
    <% end %>
